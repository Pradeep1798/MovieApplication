{
  "compilerOptions": {
    "target": "esnext",
    "module": "node16",
    "lib": ["esnext", "dom"],
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "baseUrl": "src",
    "paths": {
  "public/*": ["public/*"],
  "assets/*": ["assets/*"],
  "services/*": ["services/*"],
  "utils/*": ["utils/*"],
  "screens/*": ["screens/*"],
  "components/*": ["components/*"],
  "models/*": ["models/*"]
}
  },
  "extends": "@react-native/typescript-config/tsconfig.json"
}
